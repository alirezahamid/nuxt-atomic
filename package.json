    "@typescript-eslint/parser": "^6.7.0",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nuxt": "^3.7.3",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
